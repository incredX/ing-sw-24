<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane
        fx:id="scenePane"
        prefHeight="720.0"
        prefWidth="1280.0"
        xmlns="http://javafx.com/javafx/21"
        xmlns:fx="http://javafx.com/fxml/1">
   <children>

      <ImageView
              fx:id="starterCard"
              fitHeight="300.0"
              fitWidth="300.0"
              layoutX="500.0"
              layoutY="50.0"
              >
         <Image
                 url="@graphicResources/codexLogin.jpg" />
      </ImageView>

      <ImageView
              fx:id="goalCard1"
              fitHeight="400.0"
              fitWidth="400.0"
              layoutX="200.0"
              layoutY="300.0">
         <Image
                 url="@graphicResources/codexCards/croppedCards/croppedFront/goalFront/O2MM_.jpg" />
      </ImageView>

      <ImageView
              fx:id="goalCard2"
              fitHeight="400.0"
              fitWidth="400.0"
              layoutX="800.0"
              layoutY="300.0">
         <Image
                 url="@graphicResources/codexCards/croppedCards/croppedFront/goalFront/O2AAAD1.jpg" />
      </ImageView>
      <Button
              fx:id="readyButton"
              layoutX="871.0"
              layoutY="100.0"
              mnemonicParsing="false"
              prefHeight="50.0"
              prefWidth="200.0"
              text="Ready">
         <cursor>
            <Cursor
                    fx:constant="DEFAULT" />
         </cursor>
         <font>
            <Font
                    size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
